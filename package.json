{
  "name": "@marcuth/lsof",
  "version": "0.0.1",
  "description": "Structured output enforcement layer for LLMs without native schema support",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "!/**/__tests__"
  ],
  "scripts": {
    "tests": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "llm structured output",
    "lsof",
    "gemini like",
    "ai studio like"
  ],
  "author": "Marcuth",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "jsonrepair": "^3.13.2",
    "winston": "^3.19.0"
  },
  "peerDependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-sort-imports": "^1.8.9",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  }
}
